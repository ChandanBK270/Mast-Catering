<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    tools:context=".first1">


    <TextView
        android:id="@+id/one"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="796dp"
        android:gravity="center"
        android:text="Please Place your order"
        android:textColor="@color/black"
        android:textSize="29dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/at"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="748dp"
        android:gravity="center"
        android:text="At"
        android:textColor="@color/black"
        android:textSize="29dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/KC"
        android:layout_width="397dp"
        android:layout_height="55dp"
        android:layout_marginBottom="700dp"
        android:gravity="center"
        android:text="Karunaadu caterers"
        android:textColor="@color/black"
        android:textSize="29dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="-4dp"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/Name"
        android:layout_width="377dp"
        android:layout_height="64dp"
        android:layout_marginBottom="604dp"
        android:text="Enter Name"
        android:textColor="@color/black"
        android:textSize="29dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="8dp" />

    <EditText
        android:id="@+id/EntName"
        android:layout_width="394dp"
        android:layout_height="65dp"
        android:layout_marginBottom="536dp"
        android:background="#ECE8E8"
        android:hint="Name"
        android:textColor="@color/black"
        android:textSize="29dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="0dp" />


    <TextView
        android:id="@+id/food"
        android:layout_width="377dp"
        android:layout_height="61dp"
        android:layout_marginBottom="452dp"
        android:text="Food Type"
        android:textColor="@color/black"
        android:textSize="29dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="8dp" />


   <TextView
       android:id="@+id/attendees"
        android:layout_width="381dp"
        android:layout_height="68dp"
        android:layout_marginBottom="300dp"
        android:text="Number of attendees"
        android:textColor="@color/black"
        android:textSize="29dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="4dp" />


    <EditText
        android:id="@+id/range1"
        android:layout_width="135dp"
        android:layout_height="61dp"
        android:layout_marginBottom="252dp"
        android:background="#ECE8E8"
        android:textColor="@color/black"
        android:gravity="center"
        android:hint="0"
        android:textSize="29dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/range2"
        android:layout_width="133dp"
        android:layout_height="69dp"
        android:layout_marginBottom="244dp"
        android:background="#ECE8E8"
        android:gravity="center"
        android:textColor="@color/black"
        android:hint="N"
        android:textSize="29dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/Loc"
        android:layout_width="375dp"
        android:layout_height="61dp"
        android:layout_marginBottom="172dp"
        android:text="Event Location"
        android:textColor="@color/black"
        android:textSize="29dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="4dp" />

    <Spinner
        android:id="@+id/spinner_languages"
        android:layout_width="382dp"
        android:layout_height="68dp"
        android:layout_marginBottom="384dp"
        android:background="#ECE8E8"
        android:spinnerMode="dropdown"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.454"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/URL"
        android:layout_width="370dp"
        android:layout_height="67dp"
        android:layout_marginBottom="104dp"
        android:background="#ECE8E8"
        android:textColor="@color/black"
        android:gravity="center"
        android:hint="URL"
        android:textSize="29dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="4dp" />

    <Button
        android:id="@+id/proceed"
        android:layout_width="194dp"
        android:layout_height="67dp"
        android:layout_marginBottom="16dp"
        android:text="Proceed"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.809"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>

